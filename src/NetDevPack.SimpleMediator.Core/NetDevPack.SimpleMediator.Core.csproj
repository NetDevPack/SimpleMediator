<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>netstandard2.1</TargetFrameworks>
		<Nullable>enable</Nullable>
		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
		<AssemblyName>NetDevPack.SimpleMediator</AssemblyName>
		<RootNamespace>NetDevPack.SimpleMediator</RootNamespace>
		<PackageId>NetDevPack.SimpleMediator</PackageId>
		<Authors>Eduardo Pires</Authors>
		<Company>desenvolvedor.io</Company>
		<Description>Simple mediator implementation in .NET, In-process messaging with no dependencies.</Description>
		<PackageDescription>Simple mediator implementation in .NET, In-process messaging with no dependencies.</PackageDescription>
		<PackageProjectUrl>https://github.com/EduardoPires/NetDevPack</PackageProjectUrl>		
		<PackageIcon>IconNuget.png</PackageIcon>
		<RepositoryUrl>https://github.com/NetDevPack/SimpleMediator</RepositoryUrl>
		<RepositoryType>Public</RepositoryType>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Version>1.0.0</Version>
		<Title>NetDevPack.SimpleMediator</Title>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageTags>Mediator, MediatR</PackageTags>
	</PropertyGroup>
	
	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="6.0.0" />
	</ItemGroup>

	<ItemGroup>
		<None Include="..\..\assets\IconNuget.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
		<None Include="..\..\README.md">
		  <Pack>True</Pack>
		  <PackagePath>\</PackagePath>
		</None>
	</ItemGroup>
</Project>
